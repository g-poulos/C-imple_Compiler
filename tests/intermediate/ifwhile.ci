program ifWhile
{
    declare c,a,b,t;
    a:=1;
    while (a+b<1 and b<5)
    {
        if (t=1)
            c:=2;
        else
            if (t=2)
                c:=4;
            else
                c:=0;;;
        while (a<1)
            if (a=2)
                while(b=1)
                    c:=2;;;
    }
}.